[Unit]
Description=Chloe Orchestrator Slow Mode (hourly cadence)
After=network.target

[Service]
Type=oneshot
WorkingDirectory=/root/Chloe-alpha
Environment="PYTHONPATH=/root/Chloe-alpha"
EnvironmentFile=/root/Chloe-alpha/.env
ExecStart=/root/Chloe-alpha/venv/bin/python3 -m tools.chloe_orchestrator slow

